{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "baseUrl": "./",
    "paths": {
      "autochek-device/*": [
        "src/autochek-device/*"
      ],
      "autochek-base/*": [
        "./src/autochek-base/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "types": ["node"],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
